{
    "version": 2,
    "framework": null,
    "buildCommand": "npm run build:ssr",
    "outputDirectory": "dist/frontend/browser",
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/(.*\\.[a-z0-9]+$)",
            "destination": "/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}