{
    "version": 2,
    "framework": null,
    "buildCommand": "npm run build:ssr && npm run postbuild:ssr",
    "outputDirectory": "dist/frontend/browser",
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*\\.(?:js|css|ico|json|webp|jpg|jpeg|png|svg|woff|woff2)$)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/server/main.js"
        }
    ]
}