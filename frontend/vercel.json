{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "ng build --configuration=production",
                "outputDirectory": "dist/frontend"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/frontend/server/main.js"
        }
    ]
}